# Run `chainlit create-secret` to create one if you don't have one handy
CHAINLIT_AUTH_SECRET=

# In production environments, set this as the URL your application is visible at. (Base URL)
CHAINLIT_URL=http://localhost:8051
CHAINLIT_PORT=8051

# Set to "production" once deployed to a server, otherwise unset
DEPLOYMENT_MODE=production

# Details of your Langfuse instance
LANGFUSE_HOST=http://localhost:1235
LANGFUSE_PUBLIC_KEY=lf_pk_1234567890
LANGFUSE_SECRET_KEY=lf_sk_1234567890

# Base URL to your Metabase installation without trailing slash
METABASE_URL=http://localhost:3000
# Generated from your metabase instance
METABASE_API_KEY=

# According to our OpenAI/-compatible APIs
OPENAI_API_BASE=
OPENAI_API_KEY=

# Database for Persisting Chats
# This is only needed when you require chat history
# LIT_DATABASE_URL=postgresql+asyncpg://sense_user:password@localhost:5432/sense

# AWS Cognito
OAUTH_COGNITO_CLIENT_ID=
OAUTH_COGNITO_CLIENT_SECRET=
OAUTH_COGNITO_DOMAIN=<something>.auth.<region>.amazoncognito.com

# Define the choice of environment (dev, stg, prd)
ENVIRONMENT=dev

# Rate Limits
RATE_LIMIT_ENABLED=false
RATE_LIMIT_COUNT=10
# in seconds
RATE_LIMIT_INTERVAL=300

# Cron service (for use in containers only)
ENABLED_CRON=false

# Default to cognito if unset, change to SenseAdminAuthorizationProvider if needed
DB_WHITELIST_PROVIDER=SenseAdminAuthorizationProvider
SENSE_ADMIN_BASE_URL=http://localhost:1234
SENSE_ADMIN_API_KEY=

DEFAULT_LLM=gpt-4o

# E2E tests
SENSE_BASE_URL=http://localhost:8051
# requires a test cognito user that can be created from cognito
E2E_USERNAME=
E2E_PASSWORD=
